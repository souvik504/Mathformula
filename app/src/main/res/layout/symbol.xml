<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="#add8e6">

    <Button
        android:layout_marginTop="38dp"
        android:textColor="#fff"
        android:background="@drawable/buttons"

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/A_s"
        android:text="
বীজগণিতের প্রতীকসমূহ"
        android:layout_marginLeft="2dp"
        android:layout_marginRight="2dp"

        />
    <Button

        android:textColor="#fff"
        android:background="@drawable/buttons"
        android:layout_margin="2dp"

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/b_m_s"
        android:text="মৌলিক গণিতের প্রতীকসমূহ"

        android:layout_marginLeft="18dp"
        android:layout_marginRight="18dp"/>
    <Button

        android:textColor="#fff"
        android:background="@drawable/buttons"
        android:layout_margin="2dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/c_a_s"
        android:text="
ক্যালকুলাস  প্রতীকসমূহ"

        android:layout_marginLeft="18dp"
        android:layout_marginRight="18dp"/>
    <Button

        android:textColor="#fff"
        android:background="@drawable/buttons"
        android:layout_margin="2dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/c_s"
        android:text="সংযুক্তকারিতা প্রতীকসমূহ"

        android:layout_marginLeft="18dp"
        android:layout_marginRight="18dp"/>
    <Button

        android:textColor="#fff"
        android:background="@drawable/buttons"
        android:layout_margin="2dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/g_s"
        android:text="জ্যামিতিক চিহ্নসমূহ"

        android:layout_marginLeft="18dp"
        android:layout_marginRight="18dp"/>
    <Button

        android:textColor="#fff"
        android:background="@drawable/buttons"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        android:id="@+id/g_a_s"
        android:text="
গ্রীক বর্ণমালা প্রতীকসমূহ"
        android:layout_margin="2dp"
        android:layout_marginLeft="18dp"
        android:layout_marginRight="18dp"/>
    <Button

        android:textColor="#fff"
        android:background="@drawable/buttons"

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/L_a_s"
        android:text="রৈখিক বীজগাণিতিক প্রতীকসমূহ"
        android:layout_margin="2dp"

        android:layout_marginLeft="18dp"
        android:layout_marginRight="18dp"/>
    <Button

        android:textColor="#fff"
        android:background="@drawable/buttons"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/L_s"
        android:text="
যুক্তি চিহ্নসমূহ"
        android:layout_margin="2dp"
        android:layout_marginLeft="18dp"
        android:layout_marginRight="18dp"/>
    <Button
        android:textColor="#fff"
        android:background="@drawable/buttons"
        android:layout_margin="2dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/R_s"
        android:text="রোমান সংখ্যাসমূহ"
        android:layout_marginLeft="18dp"
        android:layout_marginRight="18dp"/>
    <Button

        android:textColor="#fff"
        android:background="@drawable/buttons"
        android:layout_margin="2dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/s_s"
        android:text="সেট তত্ত্ব প্রতীকসমূহ "

        android:layout_marginLeft="18dp"
        android:layout_marginRight="18dp"/>








</LinearLayout>